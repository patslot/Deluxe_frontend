<div id="home-slideshow-block" class="carousel slide" data-ride="carousel">
  <!-- Wrapper for slides -->
  <div class="carousel-inner" id="homempm">
<% mpms.forEach(function(mpm, idx) { %>
  <% var active = ""; if (idx === 0) { active = "active" } %>
    <div class="item <%= active %>">
      <a href="<%= mpm.linkURL %>" target="<%= mpm.linkTarget %>">
        <%- include('imageHasVideo.ejs', {a: {hasVideo: mpm.hasVideo, image: mpm.imgName}}) %>
      </a>
      <div class="text-center mpm_title">
        <div class="nm_section_block_title_cat"></div>
        <div class="nm_section_block_title">
          <a href="<%= mpm.linkURL %>" target="<%= mpm.linkTarget %>">
            <%- mpm.content %></a>
        </div>
      </div>
    </div>
<% }) %>
  </div>
</div>
